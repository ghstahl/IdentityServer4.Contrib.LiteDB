<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <RootNamespace>IdentityServer4.LiteDB</RootNamespace>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <PackageLicenseUrl>https://github.com/ghstahl/IdentityServer4.Contrib.LiteDB/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/ghstahl/IdentityServer4.Contrib.LiteDB</PackageProjectUrl>
    <Authors>ghstahl</Authors>
    <Company />
    <Copyright>Copyright ghstahl 2018</Copyright>
    <PackageTags>IdentityServer4 LiteDB</PackageTags>
    <Version>1.0.1</Version>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DocumentationFile>bin\Debug\netstandard2.0\IdentityServer4.Contrib.LiteDB.xml</DocumentationFile>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DocumentationFile>bin\Release\netstandard2.0\IdentityServer4.Contrib.LiteDB.xml</DocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="AutoMapper" Version="7.0.1" />
    <PackageReference Include="IdentityServer4" Version="2.2.0" />
    <PackageReference Include="LiteDB" Version="4.1.4" />
  </ItemGroup>

</Project>
